/*** KORA OBJECT STYLING ***/
.kora-objs {
	width: 100%;
	margin: 10px 0 0 0;
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
}
	.kora-obj {
		position: relative;
		width: 100%;
		height: 144px;
		border: 1px solid #dedede;
		cursor: pointer;
		margin: 10px 0;
		box-sizing: border-box;
	}
		.kora-obj .kora-obj-left {
			position: absolute;
			top: 0;
			left: 0;
			bottom: 0;
			width: 142px;
			border-right: 1px solid #dedede;
		}
		.kora-obj-active .kora-obj-left {
			border: 5px solid #57abce !important;
		}
			.kora-obj-left > img {
				position: absolute;
				width: 100%;
				height: 100%;
				z-index: 1;
			}
			.kora-obj-left > input {
				height: 41px;
				position: absolute;
				bottom: 0;
				right: 0;
				left: 0;
				z-index: 5;
				padding: 15px 14px 13px 14px;
				border-top: 1px solid #dedede;
				border-left: 0;
				border-right: 0;
				border-bottom: 0;
				cursor: pointer;
				transition: all 150ms linear 0s;
			}
			.kora-obj-active .kora-obj-left > input {
				border-top: 1px solid #57abce !important;
				background-color: #57abce !important;
				color: white !important;
			}
		.kora-obj .kora-obj-right {
			position: absolute;
			top: 0;
			left: 142px;
			right: 0;
			bottom: 0;
		}
		.kora-obj-active .kora-obj-right {
			border: 5px solid #57abce !important;
		}
			.kora-obj-close {
				position: absolute;
				top: 0;
				right: 0;
				width: 23px;
				height: 23px;
				z-index: 5;
				border-left: 1px solid #dedede;
				border-bottom: 1px solid #dedede;
				display: flex;
				align-items: center;
				justify-content: center;
				box-sizing: border-box;
				transition: all 150ms linear 0s;
			}
			.kora-obj-close:hover {
				border-left: 1px solid #57abce;
				border-bottom: 1px solid #57abce;
			}
			.kora-obj-fields {
				box-sizing: border-box;
				padding: 11px;
				margin: 0;
				position: absolute;
				top: 0;
				bottom: 0;
				width: 100%;
				height: 100%;
				display: flex;
				justify-content: space-between;
				flex-direction: column;
			}
				.kora-obj-fields > li {
					margin: 0;
					font-size: 11px;
					text-overflow: ellipsis;
					overflow: hidden;
					white-space: nowrap;
				}
					.kora-obj-fields > li > span {
						color: #8c8c8c;
					}
	.kora-obj:hover {
		border: 1px solid #57abce;
	}
		.kora-obj:hover .kora-obj-left {
			border-right: 1px solid #57abce;
		}
			.kora-obj:hover .kora-obj-left > input {
				border-top: 1px solid #57abce;
			}
	.kora-obj-left > input:hover {
		background-color: #57abce;
		color: white;
	}
#remove-kora-objs, #add-kora-objs, #selected-kora-objs {
	position: fixed;
	z-index: 99999; /* Chosen boxes have z-index of about 1000, and this is big enough */
	bottom: 0;
	max-width: inherit;
	padding-bottom: 10px;
	background-color: white;

    -webkit-transform: translateY(100%);
    -moz-transform: translateY(100%);
    -ms-transform: translateY(100%);
    -o-transform: translateY(100%);
    transform: translateY(100%);

    -webkit-transition: all 300ms ease 0s;
    -moz-transition: all 300ms ease 0s;
    -ms-transition: all 300ms ease 0s;
    -o-transition: all 300ms ease 0s;
    transition: all 300ms ease 0s;
}
#remove-kora-objs, #add-kora-objs {
	width: 100%;
}
#selected-kora-objs {
  right: 8px;
  left: 8px;
}
#remove-kora-objs > input {
	margin-top: 0;
	background-color: #df7171;
	border: 1px solid #df7171;
	color: white;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}
#add-kora-objs > input, #selected-kora-objs > input {
	margin-top: 0;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}
.slide-remove-objs, .slide-selected-objs {
    -webkit-transform: translateY(0) !important;
    -moz-transform: translateY(0) !important;
    -ms-transform: translateY(0) !important;
    -o-transform: translateY(0) !important;
    transform: translateY(0) !important;
}
