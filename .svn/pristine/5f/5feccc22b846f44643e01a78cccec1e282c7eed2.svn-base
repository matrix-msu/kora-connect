}

div#addNewObject.wrap div#pagination div#pageNums span {
	padding: 0 10px;
	color: #dedede;
	cursor: pointer;
}

div#addNewObject.wrap div#pagination div#pageNums span.active-page {
	color: #8c8c8c;
}

/* 'Add Object(s) to Library' Button */
div#addNewObject.wrap div#add-kora-objs input:hover {
	background: #3894bb;
}

/********* Responsiveness *********/
@media( max-width: 945px ) {
	div#addNewObject.wrap div.kora-objs div.kora-obj:nth-child(odd),
	div#addNewObject.wrap div.kora-objs div.kora-obj:nth-child(even) {
		width: 100%;
		float: none;
