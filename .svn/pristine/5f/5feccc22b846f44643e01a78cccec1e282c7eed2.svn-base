/****** Globals ******/
html {
	background-color: #ffffff;
}

#wpbody {
    font-family: "Helvetica Neue", Helvetica, sans-serif;
    font-size: 14px;
    color: #454545;
}

b, strong {
    font-family: "Open Sans Bold", "Arial Bold", sans-serif;
}

/*** Page Wrapper ***/
.wrap {
	position: relative;
}

@media only screen and (min-width: 942px) {
	.wrap {
		max-width: 864px;
	}
}

@media only screen and (min-width: 1385px) {
	.wrap {
		max-width: 1306px;
	}
}

/*** Page Titles ***/
h1 {
	padding: 0 !important;
    margin: 25px 0 40px 0 !important;
	color: #454545;
    font-size: 60px !important;
    line-height: 60px !important;
    font-family: "Helvetica Neue-UltraLight", "Helvetica Neue", Helvetica, sans-serif !important;
    font-weight: normal !important;
}

/*** Section Titles ***/
h2 {
    margin: 20px 0 !important;
	color: #454545;
    font-size: 20px !important;
    line-height: 20px !important;
    font-family: "Helvetica Neue-Light", "Helvetica Neue", Helvetica, sans-serif !important;
    font-weight: normal !important;
}

/*** Other Headers ***/
h3, h4, h5, h6 {
    font-family: "Helvetica Neue", Helvetica, sans-serif !important;
}

/*** Regular Text ***/
p {
	color: #454545;
    font-size: 13px;
    font-family: "Helvetica Neue", Helvetica, sans-serif;
    font-weight: normal;
}

/*** Inputs ***/
input[type=text], input[type=button], input[type=submit], input[type=search]{
  width: 100%;
  margin: 0;
  padding: 18px 20px;
  font-family: "Helvetica Neue", Helvetica, sans-serif;
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
  color: #454545;
  font-size: 13px;
  border: 1px solid #dedede;
  outline: none;
}

input[type=button], input[type=submit], button {
	cursor: pointer;
    -webkit-transition: all ease 0.3s;
    -moz-transition: all ease 0.3s;
    -ms-transition: all ease 0.3s;
    -o-transition: all ease 0.3s;
    transition: all ease 0.3s;
}

input[type=button] {
    color: #57abce;
    text-transform: uppercase;
    background: white;
    border: 1px solid #57abce;
}

input[type=submit] {
    margin-top: 40px;
    color: white;
    text-transform: uppercase;
    background: #57abce;
    border: 1px solid #57abce;
    cursor: pointer;
}

/*** Placeholder Text ***/
input::-webkit-input-placeholder {
	font-family: "Helvetica Neue-Light", Helvetica, sans-serif;
    color: #b3b3b3;
}

input:-ms-input-placeholder {
	font-family: "Helvetica Neue-Light", Helvetica, sans-serif;
    color: #b3b3b3;
}

input::-moz-placeholder {
	font-family: "Helvetica Neue-Light", Helvetica, sans-serif;
    color: #b3b3b3;
	opacity: 1;
}

input:-moz-placeholder {
	font-family: "Helvetica Neue-Light", Helvetica, sans-serif;
    color: #b3b3b3;
	opacity: 1;
}

input[type=search]::-webkit-input-placeholder {
	font-family: "Helvetica Neue", Helvetica, sans-serif;
	font-style: normal;
	color: #454545;
}

input[type=search]:-ms-input-placeholder {
	font-family: "Helvetica Neue", Helvetica, sans-serif;
	font-style: normal;
	color: #454545;
}

input[type=search]::-moz-placeholder {
	font-family: "Helvetica Neue", Helvetica, sans-serif;
	font-style: normal;
	color: #454545;
	opacity: 1;
}

input[type=search]:-moz-placeholder {
	font-family: "Helvetica Neue", Helvetica, sans-serif;
	font-style: normal;
	color: #454545;
	opacity: 1;
}

/*** Hover and Focus Effects ***/
input[type=text]:hover, input[type=search]:hover,
input[type=text]:focus, input[type=search]:focus,
input[type=checkbox]:focus {
    border: 1px solid #8c8c8c;
    position: relative;
    z-index: 100;

	-webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;
}

/*** Remove Default Search Input Styling ***/
input[type=search]::-webkit-search-decoration,
input[type=search]::-webkit-search-cancel-button,
input[type=search]::-webkit-search-results-button,
input[type=search]::-webkit-search-results-decoration {
    display: none;
}

/*** Buttons ***/
button {
    font-size: 13px;
    line-height: 13px;
    font-family: "Helvetica Neue", Helvetica, sans-serif;
    cursor: pointer;
    outline: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
}

.blue-btn {
	color: #fff !important;
	background-color: #57abce !important;
	border: 1px solid #57abce !important;

    -webkit-transition: background-color 150ms linear 0s;
    -moz-transition: background-color 150ms linear 0s;
    -ms-transition: background-color 150ms linear 0s;
    -o-transition: background-color 150ms linear 0s;
    transition: background-color 150ms linear 0s;
}
.blue-btn:hover {
	background-color: #3894bb !important;
}

/*********************************
 Connect Page
*********************************/
h2#koraUrlHeader {
	margin-top: 0 !important;
}

h2#ptsHeader {
    display: inline-block;
	margin: 40px 25px 0 0 !important;
    opacity: 0.2;
}

div#questions {
    display: inline-block;
    margin: 10px 0 20px 0;
    opacity: 0.2;
}

div#questions span {
    padding-right: 10px;
    color: #b3b3b3;
    font-size: 11px;
}

div#questions span:hover {
    color: #8c8c8c;
}

/*** Project boxes ***/
div#projectBox, div.newProjectBox {
	margin-bottom: 20px;
}

div#projectBox div.chosen-container {
	margin-right: -5px;
}

div.newProjectBox div.chosen-container {
    margin-right: -1px;
}

div#projectBox div.chosen-container-multi,
div.newProjectBox div.chosen-container-multi {
    margin-top: -1px;
}

/*** Inputs ***/
div.chosen-container-single, input#koraToken {
	height: 55px;
    display: inline-block;
	vertical-align: top;
    opacity: 0.2;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

div.chosen-container-single {
    width: 47.54% !important;
}

input#koraToken {
    width: 47.57%;
	overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

div.chosen-container-multi {
    width: 95% !important;
	min-height: 55px;
    display: inline-block;
    opacity: 0.2;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

/* Chosen Placeholders */
div.chosen-container-single a.chosen-default span,
div.chosen-container-multi ul.chosen-choices li.search-field input.default {
    color: #b3b3b3;
}

/* X */
img.deleteBox {
    width: 5.05%;
	height: 112px;
    float: right;
    margin-left: -1px;
    padding: 0 1.5%;

    background: white;
    border: 1px solid #dedede;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
	opacity: 0.2;
}

img.deleteBox:hover {
    border: 1px solid #8c8c8c;
    position: relative;
    z-index: 100;
}

/*** Buttons ***/
input#addNewButton {
    opacity: 0.2;
}

input#addNewButton:hover {
	background: #f4fcff;
}

input#updateConnection:hover {
	background: #3894bb;
}

/********* Responsiveness *********/
@media( max-width: 700px ) {
	div.chosen-container-single, input#koraToken, div.chosen-container-multi {
		width: 93% !important;
		display: inline-block;
	}

    input#koraToken {
        margin-top: -1px;
    }

	img.deleteBox {
		width: 7.05%;
	}
}

@media( max-width: 500px ) {
	div.chosen-container-single, input#koraToken, div.chosen-container-multi {
		width: 91% !important;
	}

	img.deleteBox {
		width: 9.05%;
	}
}


