/***** Add New Kora Object - "Edit Details" Modal *****/
.remodal-wrapper.remodal-is-opened.largeModal {
    top: 10%;
}

div#editObjectModal.remodal {
    padding: 15px 20px 20px 20px;
}

div#editObjectModal h2 {
    display: inline-block;
    margin: 0 0 15px 15px !important;
    vertical-align: top;
}

/* < Image */
div#editObjectModal img#backArrow {
    padding-right: 5px;
    cursor: pointer;
}

/* X Image */
div#editObjectModal button.remodal-close {
    top: 0;
    right: 0;
    padding: 24.5px;
    border-left: 1px solid #dedede;
}

/* Object Image */
div#editObjectModal img#objectImage {
    width: 104.3%;
    height: 392px;
    display: block;
	object-fit: cover;
	-o-object-fit: cover;
    margin: 0 0 20px -19px;
}

/*** Checkboxes ***/
div#editObjectModal form input[type=checkbox] {
    margin: 0 5px 0 0;
	border: solid 1px #dedede;
    outline: none;

    -webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;

    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
}

div#editObjectModal form input[type=checkbox]:checked:before {
    content: "";
	color: transparent;
}

div#editObjectModal form input[type=checkbox]:checked {
    background: #454545;
}

/* First Two Checkboxes */
div#editObjectModal form input[type=checkbox]:nth-child(1),
div#editObjectModal form input[type=checkbox]:nth-child(3) {
    margin-bottom: 40px;
}

/*** Labels and Descriptions ***/
div#editObjectModal form label {
    color: #8c8c8c;
    font-size: 16px;
    font-weight: normal;
    font-family: "Helvetica Neue", Helvetica, sans-serif;
}

div#editObjectModal form label.activeLabel {
    color: #454545;
}

div#editObjectModal form p {
	color: #8c8c8c;
    margin: 2px 0 20px 25px;
}

div#editObjectModal form p.activeParagraph {
    color: #454545;
}

/* First Two Labels */
div#editObjectModal form label:nth-child(2),
div#editObjectModal form label:nth-child(4) {
	display: inline-block;
    margin-bottom: 40px;
}

div#editObjectModal form label:nth-child(2) {
    margin-right: 40px;
}

/*** Button ***/
div#editObjectModal form input[type=submit] {
    margin-top: 20px;
}

div#editObjectModal form input[type=submit]:hover {
    background: #3894bb;
}


